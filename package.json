{
  "name": "qr-scanner-cli",
  "version": "0.2.2-development",
  "description": "",
  "main": "dist/index.js",
  "bin": {
    "qrscanner": "./qrscanner"
  },
  "scripts": {
    "start": "./qrscanner",
    "linter": "eslint .",
    "test": "jest",
    "test:unit": "jest src/**/*.test.ts",
    "test:integration": "jest tests/integration/cli.test.ts",
    "test:all": "jest",
    "test:coverage": "nyc --reporter=lcov npm run test:all && cat ./coverage/lcov.info | coveralls",
    "test:diff": "jest --no-cache --passWithNoTests --collectCoverage=false --findRelatedTests",
    "build:doc": "typedoc",
    "build": "tsc",
    "semantic-release": "semantic-release"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "boxen": "^4.2.0",
    "chalk": "^4.1.0",
    "clipboardy": "^2.3.0",
    "common-tags": "^1.8.0",
    "jimp": "^0.16.1",
    "meow": "^8.1.0",
    "open": "^7.3.0",
    "qrcode-reader": "^1.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/travis-cli": "^11.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/common-tags": "^1.8.0",
    "@types/jest": "^26.0.19",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-editorconfig": "^3.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "execa": "^5.0.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-ts-auto-mock": "^1.0.12",
    "lint-staged": "^10.5.3",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.0",
    "source-map-support": "^0.5.19",
    "ts-auto-mock": "^3.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tsc-prog": "^2.2.1",
    "ttypescript": "^1.5.12",
    "typedoc": "^0.20.19",
    "typedoc-plugin-markdown": "^3.4.3",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/victorperin/qr-scanner-cli.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "./**/*.js": [
      "eslint",
      "npm run test:diff"
    ]
  },
  "engines": {
    "node": ">=10"
  }
}
