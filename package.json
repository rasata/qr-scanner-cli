{
  "name": "qr-scanner-cli",
  "version": "0.2.2-development",
  "description": "",
  "main": "index.js",
  "bin": {
    "qrscanner": "./qrscanner"
  },
  "scripts": {
    "start": "./qrscanner",
    "linter": "eslint .",
    "test": "jest",
    "test:unit": "jest src/**/*.test.ts",
    "test:integration": "jest tests/integration/cli.test.ts",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:coverage": "nyc --reporter=lcov npm run test:all && cat ./coverage/lcov.info | coveralls",
    "test:diff": "jest --no-cache --passWithNoTests --collectCoverage=false --findRelatedTests",
    "build": "tsc",
    "semantic-release": "semantic-release",
    "coverage": "http-server coverage/lcov-report/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "boxen": "^4.2.0",
    "chalk": "^4.1.0",
    "clipboardy": "^2.3.0",
    "common-tags": "^1.8.0",
    "jimp": "^0.16.1",
    "meow": "^7.0.0",
    "open": "^7.3.0",
    "qrcode-reader": "^1.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/travis-cli": "^8.3.5",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "coveralls": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "execa": "^4.1.0",
    "http-server": "^0.12.1",
    "husky": "^4.3.0",
    "jest": "^25.3.0",
    "lint-staged": "^10.5.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "semantic-release": "^17.2.3",
    "source-map-support": "^0.5.16",
    "ts-jest": "^26.4.4",
    "ts-node": "^8.8.2",
    "tsc-prog": "^2.2.1",
    "typescript": "^3.7.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/victorperin/qr-scanner-cli.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "./**/*.js": [
      "eslint",
      "npm run test:diff"
    ]
  },
  "engines": {
    "node": ">=10"
  }
}
