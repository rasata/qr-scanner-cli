    "jest": "^27.0.4",
    "jest-extended": "^0.11.5",
    "jest-ts-auto-mock": "^2.0.0",
    "lint-staged": "^11.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.1",
    "semantic-release": "^17.4.3",
    "source-map-support": "^0.5.19",
    "ts-auto-mock": "^3.2.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tsc-prog": "^2.2.1",
    "ttypescript": "^1.5.12",
    "typedoc": "^0.20.36",
    "typedoc-plugin-markdown": "^3.9.0",
    "typescript": "^4.3.2"
