    "jimp": "^0.16.1",
    "open": "^8.2.0",
    "qrcode-reader": "^1.0.4",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/common-tags": "^1.8.0",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "coveralls": "^3.1.0",
